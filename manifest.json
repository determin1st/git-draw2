{
    "name": "Git Draw 2",
    "description": "Allows you to draw in your GitHub heatmap",
    "version": "2.0",
    "manifest_version": 2,
    "icons":
    {
        "16":  "icons/icon16.png",
        "48":  "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://github.com/*"],
            "js":  ["contentscript.js"],
            "css": ["contentscript.css"]
        }
    ],
    "permissions": []
}
